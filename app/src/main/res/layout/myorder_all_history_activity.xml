                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_all_history_9"
                        android:layout_marginLeft="@dimen/dimen_15"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:layout_toRightOf="@+id/tv_all_history_10"
                        android:text=""
                        android:textColor="@color/font_black_1"
                        android:textSize="@dimen/dimen_14"/>
                    <TextView
                        android:id="@+id/orderState"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/orderNum"
                        android:layout_marginLeft="@dimen/dimen_15"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:layout_toRightOf="@+id/tv_all_history_11"
                        android:text=""
                        android:textColor="@color/accent"
                        android:textSize="@dimen/dimen_14"/>
                    <TextView
                        android:id="@+id/orderTime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/orderState"
                        android:layout_marginLeft="@dimen/dimen_15"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:layout_toRightOf="@+id/tv_all_history_12"
                        android:text=""
                        android:textColor="@color/font_black_1"
                        android:textSize="@dimen/dimen_14"/>
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_5"
                    android:background="@color/base_start_color_pressed"/>

                <RelativeLayout android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_all_history_13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:text="联系人信息"
                        android:textColor="@color/text_color_1"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_all_history_14"
                        android:layout_below="@+id/tv_all_history_13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:text="联系人"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14"/>
                    <TextView
                        android:id="@+id/tv_all_history_15"
                        android:layout_below="@+id/tv_all_history_14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:text="手机号码"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14"/>
                    <TextView
