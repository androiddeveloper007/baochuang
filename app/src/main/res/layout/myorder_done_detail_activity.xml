<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">
    <ImageView
        android:id="@+id/iv_navigation_bg"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_70"
        android:scaleType="centerCrop"
        android:src="@mipmap/index_navi_bg"/>
    <LinearLayout
        android:id="@+id/view_need_offset"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/rl_navigation"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_45">
            <ImageView
                android:id="@+id/title_back"
                android:layout_width="@dimen/dimen_30"
                android:layout_height="@dimen/dimen_28"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/dimen_12"
                android:src="@drawable/backdown"
                android:visibility="visible"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="维护保养记录"
                android:textColor="@color/white"
                android:textSize="@dimen/dimen_18"/>
            <ImageView
                android:id="@+id/iv_back_home"
                android:layout_width="@dimen/dimen_25"
                android:layout_height="@dimen/dimen_25"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:layout_marginRight="@dimen/dimen_10"
                android:src="@drawable/back_home"/>
        </RelativeLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/dimen_3">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_done_detail_0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginBottom="@dimen/dimen_5"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:text=""
                        android:textColor="@color/accent"
                        android:textSize="@dimen/dimen_20"/>
                    <TextView
                        android:id="@+id/tv_done_detail_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_0"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:text="服务信息"
                        android:textColor="@color/text_color_1"
                        android:textSize="@dimen/dimen_15"/>
                    <TextView
                        android:id="@+id/tv_done_detail_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_1"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:text="保养车型"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_2_0"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:text="车牌号码"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_3"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:text="行驶里程"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_4"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:text="保养时间"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_5"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:text="服务商家"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_6"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:text="商家地址"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14.5"/>

                    <TextView
                        android:id="@+id/tv_done_detail_8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_7_0"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:text="商家电话" android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_2_0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_1"
                        android:layout_marginLeft="@dimen/dimen_8"
                        android:layout_marginRight="@dimen/dimen_10"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:layout_toRightOf="@+id/tv_done_detail_2"
                        android:maxLines="2"
                        android:text=""
                        android:textColor="@color/font_black_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_3_0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_2_0"
                        android:layout_marginLeft="@dimen/dimen_8"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:layout_toRightOf="@+id/tv_done_detail_3"
                        android:text=""
                        android:textColor="@color/font_black_2"
                        android:textSize="@dimen/dimen_14"/>
                    <TextView
                        android:id="@+id/tv_done_detail_4_0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_3"
                        android:layout_marginLeft="@dimen/dimen_8"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:layout_toRightOf="@+id/tv_done_detail_4"
                        android:text=""
                        android:textColor="@color/font_black_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_5_0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_4"
                        android:layout_marginLeft="@dimen/dimen_8"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:layout_toRightOf="@+id/tv_done_detail_5"
                        android:text=""
                        android:textColor="@color/font_black_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_6_0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_5"
                        android:layout_marginLeft="@dimen/dimen_8"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:layout_toRightOf="@+id/tv_done_detail_6"
                        android:text=""
                        android:textColor="@color/font_black_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_7_0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_6"
                        android:layout_marginLeft="@dimen/dimen_8"
                        android:layout_marginRight="@dimen/dimen_15"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:layout_toRightOf="@+id/tv_done_detail_7"
                        android:maxLines="2"
                        android:text=""
                        android:textColor="@color/font_black_2"
                        android:textSize="@dimen/dimen_14.5"/>
                    <TextView
                        android:id="@+id/tv_done_detail_8_0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_done_detail_7_0"
                        android:layout_marginLeft="@dimen/dimen_8"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:layout_toRightOf="@+id/tv_done_detail_8"
                        android:text=""
                        android:textColor="@color/font_black_2"
                        android:textSize="@dimen/dimen_14.5"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_4"
                    android:layout_marginTop="@dimen/dimen_10"
                    android:background="@color/base_start_color_pressed"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tv_done_detail_9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen_10"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_15"
                        android:text="维护清单"
                        android:textColor="@color/text_color_1"
                        android:textSize="@dimen/dimen_14.5"/>
                    <com.szbc.widget.ListView4ScrollView
                        android:id="@+id/lv_detail_products"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:divider="@null"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dimen_4"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:background="@color/base_start_color_pressed"/>
                    <TextView
                        android:id="@+id/tv_done_detail_order_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14"/>
                    <TextView
                        android:id="@+id/tv_done_detail_pay_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14"/>
                    <TextView
                        android:id="@+id/tv_done_detail_pay_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen_10"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:textColor="@color/text_color_2"
                        android:textSize="@dimen/dimen_14"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dimen_10"
                        android:layout_marginLeft="@dimen/dimen_20"
                        android:layout_marginTop="@dimen/dimen_10"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="代驾合约"
                            android:textColor="@color/primary"
                            android:textSize="@dimen/dimen_14"
                            android:visibility="visible"/>
                        <Button
                            android:id="@+id/tv_done_detail_deliver"
                            android:layout_width="@dimen/dimen_60"
                            android:layout_height="@dimen/dimen_30"
                            android:background="@drawable/button_normal"
                            android:layout_marginLeft="@dimen/dimen_35"
                            android:text="查看"
                            android:textColor="@color/white"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</RelativeLayout>